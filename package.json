{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build-client__prod && npm run server__prod",
    "start__dev": "concurrently \"npm run server__dev\" \"npm run client__dev\"",
    "server__prod": "node ./server/bin/www",
    "server__dev": "node ./server/bin/www-dev-start.js",
    "build-client__prod": "cd client && npm run build && cd ..",
    "client__dev": "node start-client__dev.js"
  },
  "dependencies": {
    "babel-preset-es2015-node6": "^0.4.0",
    "babel-register": "^6.26.0",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.3",
    "compression": "^1.7.2",
    "config": "^1.30.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.0.0",
    "express": "~4.16.0",
    "express-session": "^1.15.6",
    "express-validation": "^1.0.2",
    "helmet": "^3.12.1",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "joi": "^13.4.0",
    "mongoose": "^5.1.6",
    "morgan": "^1.9.0",
    "mysql": "^2.15.0",
    "mysql2": "^1.5.3",
    "nodemailer": "^4.6.7",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "sequelize": "^4.38.0",
    "source-map-support": "^0.5.6",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "concurrently": "^3.6.0"
  }
}
